"
https://es.wikipedia.org/wiki/Triangulaci%C3%B3n_de_Delaunay
"
Class {
	#name : #RSDelaunayExample,
	#superclass : #RSObject,
	#instVars : [
		'allParticles',
		'canvas',
		'useFill'
	],
	#category : #'Roassal3-Experimental-Delaunay'
}

{ #category : #adding }
RSDelaunayExample >> addParticle: aRSParticleD [
	allParticles add: aRSParticleD
]

{ #category : #girls }
RSDelaunayExample >> anime1 [
	| girl |
	girl := self girl1.
	canvas addShape: girl.
	girl position: 10000 asPoint.
	^ canvas transitionAnimation
		duration: 2 seconds;
		delay: 2 seconds;
		from: 0;
		to: 500;
		onStepDo: [ :t | girl translateTo: t @ 300  ];
		when: RSAnimationEndEvent do: [
			girl position: 10000 asPoint.]
]

{ #category : #girls }
RSDelaunayExample >> anime2 [
	| girl |
	girl := self girl2.
	canvas addShape: girl.
	girl position: 10000 asPoint.
	^ canvas transitionAnimation
		duration: 2 seconds;
		delay: 1 seconds;
		from: 500;
		to: 0;
		onStepDo: [ :t | girl translateTo: t @ (300+ (((t*2) degreesToRadians sin) * 10))  ];
		when: RSAnimationEndEvent do: [
			girl position: 10000 asPoint.]
]

{ #category : #girls }
RSDelaunayExample >> anime3 [
	| girl y |
	girl := self girl3.
	canvas addShape: girl.
	girl position: 10000 asPoint.
	y := NSScale linear
		domain: #(0 100 200 300 400 500);
		range: #(250 350 250 350 250 350).
	^ canvas transitionAnimation
		duration: 2 seconds;
		delay: 1 seconds;
		from: 0;
		to: 500;
		onStepDo: [ :t |

			girl translateTo: t @ (y scale:t)  ];
		when: RSAnimationEndEvent do: [
			girl position: 10000 asPoint.]
]

{ #category : #running }
RSDelaunayExample >> borderFor: particle [
	useFill ifTrue: [ ^ nil ].
	^ RSBorder new
		width: 0.1;
		color: (self colorFor: particle);
		yourself
]

{ #category : #accessing }
RSDelaunayExample >> canvas: aCanvas [
	canvas := aCanvas
]

{ #category : #running }
RSDelaunayExample >> colorFor: particle [
	^ (Color h: particle life * 1.5 + 165 s: 1 v: 1)
]

